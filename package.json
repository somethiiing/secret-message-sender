    "express": "^4.15.3",
    "react": "^15.6.1",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10",
    "react-toolbox": "^2.0.0-beta.6",
    "react-toolbox-themr": "^1.0.2"
